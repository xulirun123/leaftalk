{
  "name": "leaftalk-enterprise",
  "version": "1.0.0",
  "description": "叶语 - 企业级高性能跨平台即时通讯应用",
  "scripts": {
    "dev": "vite --host 127.0.0.1 --port 5173",
    "build": "vite build",
    "preview": "vite preview --port 1420",
    "server": "node production-server.js",
    "webrtc": "node server/start-webrtc.js",
    "start": "node scripts/start-all.js",
    "start:all": "node start-with-redis.js",
    "start:redis": "node start-with-redis.js",
    "start:simple": "concurrently \"npm run server\" \"npm run dev\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\"",
    "start:prod": "concurrently \"npm run server\" \"npm run preview\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\"",
    "stop": "node scripts/stop-all.js",
    "status": "node scripts/check-status.js",
    "reset": "node scripts/reset-database.cjs",
    "init": "node scripts/init-database.cjs",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "production": "node production-server.js",
    "stable": "node stable-unified-server.js",
    "pm2:dev": "pm2 start production.config.js --env development",
    "pm2:prod": "pm2 start production.config.js --env production",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
